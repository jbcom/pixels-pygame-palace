  "scripts": {
    "dev": "concurrently -n 'backend,server,vite' -c 'yellow,cyan,green' 'npm:dev:python' 'npm:dev:server' 'npm:dev:client'",
    "dev:python": "cd backend && python app.py",
    "dev:server": "NODE_ENV=development tsx server/index.ts",
    "dev:client": "vite",
    "build": "npm run build:client && npm run build:server && npm run build:python",
    "build:client": "vite build",
    "build:server": "esbuild server/index.ts --platform=node --packages=external --bundle --format=esm --outdir=dist",
    "build:python": "cd backend && uv sync --frozen",
    "start": "concurrently -n 'backend,server' -c 'yellow,cyan' 'npm:start:python' 'npm:start:server'",
    "start:python": "cd backend && gunicorn app:app -k eventlet -w 1 -b 0.0.0.0:5001 --timeout 120",
    "start:server": "NODE_ENV=production node dist/index.js",
    "validate:config": "tsx scripts/validate-config.ts && cd backend && python -c 'from config import SERVICE_CONFIG; print(\"âœ“ Config valid\")'",
    "check": "tsc && cd backend && mypy .",
    "test": "vitest",
    "test:python": "cd backend && pytest",
    "test:all": "concurrently -n 'js,python' -c 'cyan,yellow' 'npm:test' 'npm:test:python'"
  },
